{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ODUFlix Movies for testing JSON format",
  "type": "object",
  "properties": {
    "Name ": {
      "type": "string"
    },
    "Producer": {
      "type": "string"
    },
    "Year": {
      "type": "number"
    },
    "Classification": {
      "type": "string"
    },
    "Stars": {
      "type": "number"
    },
    "Actors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "Name ",
    "Producer",
    "Year",
    "Classification",
    "Stars",
    "Actors"
  ]
}